{
    "name": "vscode-jq",
    "displayName": "vscode-jq",
    "description": "jq plugin for Visual Studio Code",
    "version": "0.0.4",
    "publisher": "dandric",
    "engines": {
        "vscode": "^1.8.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.jq"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [{
            "command": "extension.jq",
            "title": "jq - JSON Processor"
        }]
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "dependencies": {
        "node-jq": "0.5.0"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.3",
        "mocha": "^2.3.3",
        "node-jq": "0.5.0",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    }
}